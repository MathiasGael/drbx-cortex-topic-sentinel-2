   <!-- =========================================== -->
   <!-- Under GRANULE Folder (Level-1C)             -->
   <!-- =========================================== -->

   <!-- TODO ATTENTION changement d'ID<owl:Class rdf:about="&s2;pdi_level_1c_tile_container"> -->
   <!-- Compliant with COMPACT -->
   <owl:Class rdf:about="&s2;level1cGranuleContainer">
      <rdfs:label xml:lang="en">Level-1C: Granule Container</rdfs:label>
      <rdfs:comment xml:lang="en">
         One of the granules composing the User Product Level-1C.
         There are N granules-folders each one corresponding to the Tiles
         composing the product.
         The PDI Level-1C Tile consist of: 
         1. Level-1C_Tile_Metadata_File (Tile Metadata): XML Main Metadata 
             file (DIMAP mandatory file) containing the requested level of  
             information and referring all the product elements describing the 
             tile,
         2. IMG_DATA: folder containing image data files compressed using the 
            JPEG2000 algorithm, one file per band.
         3. QI_DATA: folder containing OLQC XML reports of quality checks, mask
            files and PVI file;
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&s2;granuleContainer"/>
      <drb:signature rdf:parseType="Resource">
         <!-- NB signature before COMPACT support
         <drb:nameMatch>S2[AB]_...._MSI_L1C_TL.*_N[0-9]{2}\.[0-9]{2}</drb:nameMatch>
         -->
         <drb:nameMatch>(S2[AB]_...._MSI_L1C_TL.*_N[0-9]{2}\.[0-9]{2}|L1C_T....._A[0-9]{6}_...............)</drb:nameMatch>
         <drb:parentClassTest>&s2;userProductLevel1cGranuleFolder</drb:parentClassTest>
      </drb:signature>
   </owl:Class>

   <!-- ============================ -->
   <!-- Image Data Folder (Level-1C) -->
   <!-- ============================ -->

   <!-- TODO ATTENTION changement d'ID <owl:Class rdf:about="&s2;pdi_image_folder"> -->
   <!-- Compliant with COMPACT NO MODIFICATION REQUIRED -->
   <owl:Class rdf:about="&s2;level1cImageFolder">
      <rdfs:label xml:lang="en">Level-1C: Image Folder</rdfs:label>
      <rdfs:comment xml:lang="en">
         During the Level-1C User Product generation, according to the Spectral
         Bands download options, the IMG_DATA folder can contain a set of bands
         and/or the TCI corresponding to the Tile. The TCI can be requested
         into the User Product even if no other spectral band data has been
         selected.
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&s2;imageFolder"/>
      <drb:signature rdf:parseType="Resource">
         <drb:nameTest>IMG_DATA</drb:nameTest>
         <drb:parentClassTest>&s2;level1cGranuleContainer</drb:parentClassTest>
      </drb:signature>
   </owl:Class>

   <!-- ============================ -->
   <!-- Tile Image File (Level-1C)   -->
   <!-- ============================ -->

   <!-- TODO ATTENTION changement d'ID <owl:Class rdf:about="&s2;pdi_level_1c_image_file"> -->
   <!-- Compliant with COMPACT -->
   <owl:Class rdf:about="&s2;level1ciTileImageFile">
      <rdfs:label xml:lang="en">Level-1C: Tile Image File</rdfs:label>
      <rdfs:comment xml:lang="en">
         One JPEG2000 file per band.
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&jpeg2000;JPEG2000"/>
      <drb:signature rdf:parseType="Resource">
         <!-- NB signature before COMPACT support
         <drb:nameMatch>S2[AB]_...._MSI_L1C_.*_B(01|02|03|04|05|06|07|08|09|10|11|12|8A)\.(?i)jp2</drb:nameMatch>
         -->
         <drb:nameMatch>(S2[AB]_...._MSI_L1C_.*|T....._[0-9]{4}[0-1][0-9][0-3][0-9]T[0-2][0-9][0-6][0-9][0-6][0-9])_(B(01|02|03|04|05|06|07|08|09|10|11|12|8A)|TCI|PVI)\.(?i)jp2</drb:nameMatch>
         <drb:parentClassTest>&s2;level1cImageFolder</drb:parentClassTest>
      </drb:signature>
   </owl:Class>

   <!-- ============================ -->
   <!-- Quality Control Folder (Level-1C) -->
   <!-- ============================ -->

   <!-- Compliant with COMPACT NO MODIFICATION REQUIRED -->
   <owl:Class rdf:about="&s2;level1cQualityControlFolder">
      <rdfs:label xml:lang="en">Level-1C: Quality Control Folder</rdfs:label>
      <rdfs:comment xml:lang="en">
         Folder containing the XML reports including the quality control checks
         performed by OLQC processor (for each L0/L1A/L1B/L1C PDI) and the GML
         quality masks (for each L1A/L1B/L1C PDI). In addition, in case of L1C
         Tile, this folder contains the PVI file.
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&s2;qualityControlFolder"/>
      <drb:signature rdf:parseType="Resource">
         <drb:nameTest>QI_DATA</drb:nameTest>
         <drb:parentClassTest>&s2;level1cGranuleContainer</drb:parentClassTest>
      </drb:signature>
   </owl:Class>

   <!-- ================================= -->
   <!-- Tile Metadata Files (Level-1C)    -->
   <!-- ================================= -->

   <!-- TODO ATTENTION changement d'ID <owl:Class rdf:about="&s2;pdi_level_1c_tile_metadata"> -->
   <!-- Compliant with COMPACT -->
   <owl:Class rdf:about="&s2;level1cGranuleMetadataFile">
      <rdfs:label xml:lang="en">Level-1C: Granule Metadata File</rdfs:label>
      <rdfs:comment xml:lang="en">
         The main XML metadata file provided inside each Level-1C tile.
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&drb;xml"/>
      <drb:signature rdf:parseType="Resource">
         <drb:parentClassTest>&s2;level1cGranuleContainer</drb:parentClassTest>
         <!-- NB signature before COMPACT support
         <drb:nameMatch>S2[AB]_...._MTD_L1C_TL_.*</drb:nameMatch>
         -->
         <drb:nameMatch>(S2[AB]_...._MTD_L1C_TL_.*|MTD_TL\.xml)</drb:nameMatch>
      </drb:signature>
      <drb:schemaLocation>xsd/dimap/V12/S2_PDI_Level-1C_Tile_Metadata.xsd</drb:schemaLocation>
   </owl:Class>

   <!-- =========================================== -->
   <!-- Under DATASTRIP Folder (Level-1C)           -->
   <!-- =========================================== -->

   <!-- ================================ -->
   <!-- Datastrip Container (Level-1C)   -->
   <!-- ================================ -->

   <!-- TODO ATTENTION changement d'ID <owl:Class rdf:about="&s2;pdi_level_1c_datastrip_container"> -->
   <!-- Compliant with COMPACT -->
   <owl:Class rdf:about="&s2;level1cDatastripContainer">
      <rdfs:label xml:lang="en">Level-1C: Datastrip Container</rdfs:label>
      <rdfs:comment xml:lang="en">
         One of the datastrips for User Product Level-1C.
         The PDI Level 1C DataStrip consist of:
         1. DataStrip_Metadata_File: XML Main Metadata file (DIMAP mandatory
            file) containing the requested level of information and referring all
            the product elements describing the DataStrip.
         2. QI_DATA: folder containing XML reports providing Quality control
            checks information.
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&s2;datastripContainer"/>
      <drb:signature rdf:parseType="Resource">
         <!-- NB signature before COMPACT support
         <drb:nameMatch>S2[AB]_...._MSI_L1C_DS.*_N[0-9]{2}\.[0-9]{2}</drb:nameMatch>
         -->
         <drb:nameMatch>(S2[AB]_...._MSI_L1C_DS.*_N[0-9]{2}\.[0-9]{2}|DS_...._[0-9]{4}[0-1][0-9][0-3][0-9]T[0-2][0-9][0-6][0-9][0-6][0-9]_S[0-9]{4}[0-1][0-9][0-3][0-9]T[0-2][0-9][0-6][0-9][0-6][0-9])</drb:nameMatch>
         <drb:parentClassTest>&s2;userProductLevel1cDatastripFolder</drb:parentClassTest>
      </drb:signature>
   </owl:Class>

   <!-- ================================ -->
   <!-- Datastrip Metadata File (Level-1C) -->
   <!-- ================================ -->

   <!-- TODO ATTENTION changement d'ID <owl:Class rdf:about="&s2;pdi_level_1c_datastrip_metadata"> -->
   <!-- Compliant with COMPACT -->
   <owl:Class rdf:about="&s2;level1cDatastripMetadataFile">
      <rdfs:label xml:lang="en">Level-1C: Datastrip Metadata File</rdfs:label>
      <rdfs:comment xml:lang="en">
         XML Main Metadata file (DIMAP mandatory file) containing the requested
         level of information and referring all the product elements describing
         the datastrip.
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&drb;xml"/>
      <drb:signature rdf:parseType="Resource">
         <!-- NB signature before COMPACT support
         <drb:nameMatch>S2[AB]_...._MTD_L1C_DS_.*</drb:nameMatch>
         -->
         <drb:nameMatch>(S2[AB]_...._MTD_L1C_DS_.*|MTD_DS.*)</drb:nameMatch>
         <drb:parentClassTest>&s2;level1cDatastripContainer</drb:parentClassTest>
      </drb:signature>
      <drb:schemaLocation>xsd/dimap/V12/S2_PDI_Level-1C_Datastrip_Metadata.xsd</drb:schemaLocation>
   </owl:Class>

</rdf:RDF>
