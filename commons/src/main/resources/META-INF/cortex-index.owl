<rdf:RDF xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:drb="&drb;"
         xmlns:jpeg2000="&jpeg2000;"
         xmlns:s2="&s2;"
         xml:base="&s2;">

   <owl:Ontology rdf:about="&s2;">
      <rdfs:label xml:lang="en">Sentinel-2 Commons Ontology</rdfs:label>
   </owl:Ontology>

   <!-- =================================================================== -->
   <!--                                                                     -->
   <!-- Products                                                            -->
   <!--                                                                     -->
   <!-- =================================================================== -->

   <!-- ============================ -->
   <!-- Product                      -->
   <!-- ============================ -->

   <!-- Compliant with COMPACT -->
   <owl:Class rdf:about="&s2;product">
      <rdfs:label xml:lang="en">Sentinel-2 Product</rdfs:label>
      <rdfs:comment xml:lang="en">
         Sentinel-2 product
      </rdfs:comment>
     <rdfs:subClassOf rdf:resource="&drb;item"/>
     <rdfs:subClassOf rdf:resource="&drb;safe-product"/>
      <drb:signature rdf:parseType="Resource">
         <drb:nameMatch>S2[AB]_.*</drb:nameMatch>
      </drb:signature>
   </owl:Class>

   <!-- =========================================== -->
   <!-- Under GRANULE Folder                        -->
   <!-- =========================================== -->

   <!-- ============================ -->
   <!-- Granule Container            -->
   <!-- ============================ -->

   <!-- Compliant with COMPACT -->
   <owl:Class rdf:about="&s2;granuleContainer">
      <rdfs:label xml:lang="en">Granule Container</rdfs:label>
      <rdfs:comment xml:lang="en">
         One of the granules composing the User Product. 
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&drb;item"/>
      <drb:signature rdf:parseType="Resource">
         <!-- NB signature before COMPACT support
         <drb:nameMatch>S2[AB]_...._MSI_L(0__GR|1A_GR|1B_GR|1C_TL).*_N[0-9]{2}\.[0-9]{2}</drb:nameMatch>
         -->
         <drb:nameMatch>(S2[AB]_...._MSI_L(0__GR|1A_GR|1B_GR|1C_TL).*_N[0-9]{2}\.[0-9]{2}|L.._T....._A[0-9]{6}_...............)</drb:nameMatch>
         <drb:parentClassTest>&s2;granuleFolder</drb:parentClassTest>
      </drb:signature>
   </owl:Class>

   <!-- ============================ -->
   <!-- Image Data Folder            -->
   <!-- ============================ -->

   <!-- TODO ATTENTION changement d'ID <owl:Class rdf:about="&s2;pdi_image_folder"> -->
   <!-- Compliant with COMPACT NO MODIFICATION REQUIRED -->
   <owl:Class rdf:about="&s2;imageFolder">
      <rdfs:label xml:lang="en">Image Folder</rdfs:label>
      <rdfs:comment xml:lang="en">
         Folder containing the image data foreseen for each kind of Granule /
         Tile.
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&drb;item"/>
      <drb:signature rdf:parseType="Resource">
         <drb:nameTest>IMG_DATA</drb:nameTest>
         <drb:parentClassTest>&s2;granuleContainer</drb:parentClassTest>
      </drb:signature>
   </owl:Class>

   <!-- =========================================== -->
   <!-- Under DATASTRIP Folder                      -->
   <!-- =========================================== -->

   <!-- ============================ -->
   <!-- Datastrip Container          -->
   <!-- ============================ -->

   <!-- Compliant with COMPACT -->
   <owl:Class rdf:about="&s2;datastripContainer">
      <rdfs:label xml:lang="en">Datastrip Container</rdfs:label>
      <rdfs:comment xml:lang="en">
         One of the datastrips composing the User Product. 
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&drb;item"/>
      <drb:signature rdf:parseType="Resource">
         <drb:parentClassTest>&s2;userProductDatastripFolder</drb:parentClassTest>
         <!-- NB signature before COMPACT support
         <drb:nameMatch>S2[AB]_...._MSI_L(0__|1A_|1B_|1C_)DS.*_N[0-9]{2}\.[0-9]{2}</drb:nameMatch>
         -->
         <drb:nameMatch>(S2[AB]_...._MSI_L(0__|1A_|1B_|1C_)DS.*_N[0-9]{2}\.[0-9]{2}|DS_...._[0-9]{4}[0-1][0-9][0-3][0-9]T[0-2][0-9][0-6][0-9][0-6][0-9]_S[0-9]{4}[0-1][0-9][0-3][0-9]T[0-2][0-9][0-6][0-9][0-6][0-9])</drb:nameMatch>
      </drb:signature>
   </owl:Class>

   <!-- =========================================== -->
   <!-- Under GRANULE or DATASTRIP Folder           -->
   <!-- =========================================== -->

   <!-- ============================ -->
   <!-- Quality Control Folder       -->
   <!-- ============================ -->

   <!-- TODO ATTENTION changement d'ID <owl:Class rdf:about="&s2;pdi_quality_folder"> -->
   <!-- Compliant with COMPACT NO MODIFICATION REQUIRED -->
   <owl:Class rdf:about="&s2;qualityControlFolder">
      <rdfs:label xml:lang="en">Quality Control Folder</rdfs:label>
      <rdfs:comment xml:lang="en">
         Folder containing the XML reports including the quality control checks
         performed by OLQC processor (for each L0/L1A/L1B/L1C PDI) and the GML
         quality masks (for each L1A/L1B/L1C PDI). In addition, in case of L1C
         Tile, this folder contains the PVI file.
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&drb;item"/>
      <drb:signature rdf:parseType="Resource">
         <drb:nameTest>QI_DATA</drb:nameTest>
         <drb:parentClassTest>&s2;granuleContainer</drb:parentClassTest>
      </drb:signature>
   </owl:Class>

   <!-- ============================ -->
   <!-- Quality Control File         -->
   <!-- ============================ -->

   <!-- TODO ATTENTION changement d'ID<owl:Class rdf:about="&s2;pdi_quality_control_file"> -->
   <!-- Compliant with COMPACT -->
   <owl:Class rdf:about="&s2;qualityControlFile">
      <rdfs:label xml:lang="en">Quality Control File</rdfs:label>
      <rdfs:comment xml:lang="en">
         Quality control report file contains detected errors in this product.
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&drb;xml"/>
      <drb:signature rdf:parseType="Resource">
         <drb:parentClassTest>&s2;qualityControlFolder</drb:parentClassTest>
         <!-- NB signature before COMPACT support
         <drb:nameMatch>S2[AB]_...._.*_(?i)report\.xml</drb:nameMatch>
         -->
         <drb:nameMatch>(S2[AB]_...._.*_(?i)report|SENSOR_QUALITY|GEOMETRIC_QUALITY|GENERAL_QUALITY|RADIOMETRIC_QUALITY|FORMAT_CORRECTNESS)\.xml</drb:nameMatch>
      </drb:signature>
      <drb:schemaLocation>
          xsd/olqc-report/OLQC_Report.xsd
      </drb:schemaLocation>
   </owl:Class>

