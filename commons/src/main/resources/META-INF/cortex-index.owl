   <!-- Mask File                    -->
   <!-- ============================ -->

   <!-- TODO ATTENTION changement d'ID<owl:Class rdf:about="&s2;pdi_mask_file"> -->
   <!-- Compliant with COMPACT -->
    <owl:Class rdf:about="&s2;maskFile">
